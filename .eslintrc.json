{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "indent": "warn",
    "no-shadow": "warn",
    "spaced-comment": "warn",
    "consistent-return": "warn",
    "no-param-reassign": "off",
    "no-console": "warn",
    "no-underscore-dangle": "off",
    "no-undef": "warn",
    "node/no-unpublished-require": "off"
  }
}
